<!DOCTYPE html>
<html>
  <head>
    <title>RSS Reader</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="header">
      <h1>NY Times RSS Reader</h1>
      <%= button_to "Log Out", logout_path, method: "delete" if current_user %>
      <hr>
    </div>

    <%= yield %>

  </body>
</html>
