function news() {
  $.ajax({
    url: "http://api.nytimes.com/svc/news/v3/content/nyt/all?api-key=" + <%= ENV["nytimes_key"] %>,
    type: "GET",
    success: function(news_data) {
      console.log("success!");
    },
    error: function(message){
      console.error(message);
    }
  });
}

$(document).ready(function() {
  news();
});
